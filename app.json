{
  "mode": "Beta",
  "expo": {
    "name": "장비 콜",
    "slug": "jangbeecall_storybook",
    "privacy": "public",
    "scheme": "expo.examples.with-webbrowser-redirect",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.8.1_20200320",
    "orientation": "portrait",
    "primaryColor": "#F9C06B",
    "icon": "./assets/images/icon-512.png",
    "splash": {
      "image": "./assets/images/splash-20190712.png",
      "resizeMode": "contain",
      "backgroundColor": "#4D4A4A"
    },
    "updates": {
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "yes",
      "publishBundlePath": "ios/----/Supporting/shell-app.bundle",
      "publishManifestPath": "ios/----/Supporting/shell-app-manifest.json"
    },
    "androidStatusBar": {
      "backgroundColor": "#4D4A4A",
      "bundleIdentifier": "com.kan.jangbeecall"
    },
    "android": {
      "package": "com.kan.jangbeecall",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "READ_PHONE_STATE"
      ],
      "versionCode": 20,
      "splash": {
        "backgroundColor": "#4D4A4A",
        "resizeMode": "cover"
      },
      "publishBundlePath": "android/app/src/main/assets/shell-app.bundle",
      "publishManifestPath": "android/app/src/main/assets/shell-app-manifest.json"
    },
    "description": "",
    "isDetached": true
  }
}
